# Environment Template File
# Copy this to .env and fill in your actual values

# App Configuration
APP_NAME=Smart Mobile LPR
APP_VERSION=1.0.0
APP_ENV=development

# API Configuration
API_BASE_URL=https://api.example.com
API_TIMEOUT=30000
API_KEY=your_api_key_here

# Authentication
AUTH_TOKEN_KEY=lpr_auth_token
SESSION_TIMEOUT=3600

# Database
DB_NAME=lpr_database
DB_VERSION=1

# Camera Settings
CAMERA_QUALITY=high
CAMERA_FLASH_MODE=auto

# Detection Settings
DETECTION_CONFIDENCE_THRESHOLD=0.85
MAX_DETECTION_ATTEMPTS=3

# Storage
STORAGE_PATH=/storage/lpr
MAX_IMAGE_SIZE=5242880

# Features Flags
ENABLE_CAMERA=true
ENABLE_ANALYTICS=true
ENABLE_EXPORT=true
ENABLE_DEBUG_MODE=false

# Third Party Services
FIREBASE_API_KEY=
FIREBASE_PROJECT_ID=
FIREBASE_APP_ID=

# Logging
LOG_LEVEL=info
ENABLE_CRASH_REPORTING=true
